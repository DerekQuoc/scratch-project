{
    "manifest_version": 2,
    "name": "FamPass",
    "version": "0.1.0",
    "description": "A centralized website that allows users to share accounts without exposing their real passwords.",
    "content_scripts": [
       {
          "matches": ["<all_urls>"], 
          "js": ["script.js"], 
          "css": ["extension.css"]
       }
     ],
     "browser_action": {
        "default_popup": "popup.html"
     },
     "permissions": [
        "activeTab"
     ]
   }